spring:
  application:
    name: orderservice
  jpa:
    hibernate:
      ddl-auto: create-drop
    show-sql: true
  cloud:
    config:
      enabled: true
      uri:
      - http://192.168.1.7:8000
      fail-fast: true
server:
  port: 8001
eureka:
  client:
    register-with-eureka: true
    fetch-registry: true
    service-url:
      defaultZone: http://192.168.1.6:8761/eureka,http://192.168.1.7:8761/eureka
  instance:
    prefer-ip-address: true
    ip-address: 192.168.1.7