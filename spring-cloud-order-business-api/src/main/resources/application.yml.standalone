server:
  port: 8003
management:
  endpoints:
    web:
      base-path: /
      exposure:
        include:
        - hystrix.stream
eureka:
  client:
    register-with-eureka: true
    fetch-registry: true
    service-url:
      defaultZone: http://192.168.1.6:8761/eureka,http://192.168.1.7:8761/eureka
  instance:
    prefer-ip-address: true
feign:
  hystrix:
    enabled: true